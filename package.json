{
  "name": "audios",
  "version": "0.0.0",
  "private": true,
  "watch": {
    "start": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "build": "babel --presets es2015 -d lib/ src",
    "start": "npm run build && node lib/bin/www.js",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@tensorflow-models/speech-commands": "^0.3.9",
    "@tensorflow/tfjs": "^1.2.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "npm-watch": "^0.6.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
